{
    "name": "querize",
    "version": "1.0.6",
    "description": "Objective Query",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types" : "./esm/types/index.d.ts",
                "default": "./esm/index.js"
            },
            "require": {
                "types": "./cjs/types/index.d.ts",
                "default": "./cjs/index.cjs"
            },
            "default": {
                "types" : "./esm/types/index.d.ts",
                "default": "./esm/index.js"
            }
        },
        "./package.json": "./package.json"
    },
    "author": "lclasser",
    "license": "ISC",
    "keywords": [
        "mysql",
        "maria",
        "mariadb",
        "query",
        "querize",
        "sql",
        "objective",
        "orm"
    ],
    "main": "cjs/index.cjs",
    "module": "esm/index.js",
    "engines": {
        "node": ">=16"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/itfin-git/Querize"
    }
}
